    <Content>
        <TabPanel Name="password" Class="h-100">
            <iframe class="fullscreen-iframe" src="/Identity/Account/Manage/ChangePassword" style="border:none;"></iframe>
        </TabPanel>
    </Content>
</FullHeightTabs>